{
  "name": "noteapp",
  "version": "1.0.0",
  "main": "app/index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"{app,shared}/**/*.{js,jsx,ts,tsx}\"",
    "analyze:unused": "knip",
    "analyze:exports": "knip --include exports",
    "docs": "typedoc",
    "analyze:deps": "node_modules/.bin/depcruise --validate .dependency-cruiser.js --output-type dot app/ > dependency-graph.dot && dot -T svg dependency-graph.dot > dependency-graph.svg",
    "env:dev": "cp .env.development .env",
    "env:prod": "cp .env.production .env",
    "start:dev": "npm run env:dev && expo start",
    "start:prod": "npm run env:prod && expo start",
    "android:dev": "npm run env:dev && expo start --android",
    "android:prod": "npm run env:prod && expo start --android",
    "build:dev": "npm run env:dev && eas build --profile development",
    "build:preview": "npm run env:prod && eas build --profile preview",
    "build:prod": "npm run env:prod && eas build --profile production"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@types/diff-match-patch": "^1.0.36",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "diff-match-patch": "^1.0.5",
    "expo": "54.0.23",
    "expo-auth-session": "^7.0.8",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "^6.0.17",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-navigation-bar": "~5.0.9",
    "expo-secure-store": "^15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "^15.0.9",
    "jest-expo": "~54.0.13",
    "jszip": "^3.10.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-iap": "^14.4.38",
    "react-native-markdown-display": "^7.0.2",
    "react-native-nitro-modules": "^0.31.6",
    "react-native-reanimated": "^4.1.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.6.2",
    "@types/react": "~19.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "babel-preset-expo": "^54.0.3",
    "dependency-cruiser": "^17.0.2",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "jest-expo": "~54.0.13",
    "knip": "^5.69.1",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "29.1.2",
    "typedoc": "^0.28.14",
    "typescript": "~5.9.2"
  },
  "private": true
}
