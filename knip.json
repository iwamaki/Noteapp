{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/index.ts"
  ],
  "project": [
    "app/**/*.{ts,tsx}"
  ],
  "ignore": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/node_modules/**",
    "**/__tests__/**",
    "**/__mocks__/**"
  ],
  "ignoreDependencies": [
    "@types/*",
    "typescript",
    "eslint",
    "prettier",
    "jest",
    "@babel/*"
  ],
  "ignoreExportsUsedInFile": true
}
