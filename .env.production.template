# 本番環境用の環境変数設定
# 使用方法: cp .env.production .env

# API Base URL（本番環境）
EXPO_PUBLIC_API_BASE_URL=https://api.noteapp.iwamaki.app

# Log level: debug | info | warn | error | none
# 本番環境ではerrorレベルでエラーのみ表示
EXPO_PUBLIC_LOG_LEVEL=error

# Log categories: all | カンマ区切りのカテゴリリスト
# 本番環境ではエラー関連のみ表示
EXPO_PUBLIC_LOG_CATEGORIES=all

# LLMの表示非表示
EXPO_PUBLIC_LLM_ENABLED=true

# Google OAuth2 Client IDs
# Web Application Client ID（Authorization Code Flow用）
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=461522030982-4d1fak06lfpaq2ppol18899anposuukb.apps.googleusercontent.com

